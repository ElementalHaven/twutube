{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "node",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"jsx": "react",
		"noImplicitAny": false,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "ESNext"
	},
	"exclude": [
		"node_modules",
		"wwwroot"
	]
}
